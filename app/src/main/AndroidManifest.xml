<?xml version="1.0" encoding="utf-8"?>

<manifest xmlns:android="http://schemas.android.com/apk/res/android"
            package="com.net.semicolonexpected.PetConnect">

<uses-permission android:name="android.permission.INTERNET"/>

<application android:allowBackup="true" android:label="@string/app_name"
    android:icon="@mipmap/ic_launcher" android:theme="@style/AppTheme">

    <meta-data android:name="com.facebook.sdk.ApplicationId" android:value="@string/facebook_app_id"/>

    <activity android:name="com.facebook.FacebookActivity"
        android:configChanges=
            "keyboard|keyboardHidden|screenLayout|screenSize|orientation"
        android:theme="@android:style/Theme.Translucent.NoTitleBar"
        android:label="@string/app_name" />

    <activity android:name="com.net.semicolonexpected.PetConnect.MainActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN"/>
            <category android:name="android.intent.category.LAUNCHER"/>
        </intent-filter>
    </activity>
    <activity android:name="com.net.semicolonexpected.PetConnect.Parser"/>
    <activity android:name="com.net.semicolonexpected.PetConnect.testFrisky"/>
    <activity android:name="com.net.semicolonexpected.PetConnect.Volunteer"/>
    <activity android:name="com.net.semicolonexpected.PetConnect.AddNewPost"/>
    <activity android:name="com.net.semicolonexpected.PetConnect.Breadface"/>
    <activity android:name="com.net.semicolonexpected.PetConnect.Fido" />
</application>

</manifest>
